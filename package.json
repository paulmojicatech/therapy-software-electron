{
  "name": "therapy-software",
  "version": "1.0.0",
  "author": "Paul Mojica Technologies",
  "description": "Therapy Software",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-nest-js": "cd ../../nest-js-therapy-software-api/therapy-software-api && npm start",
    "electron": "ng build --base-href ./ && electron .",
    "package-win": "ng build --prod --base-href ./ && electron-packager . --overwrite --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "package-mac": "ng build --prod --base-href ./ && electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --electron-version=3.0.7"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "angular-calendar": "^0.26.11",
    "core-js": "^2.6.5",
    "date-fns": "^1.30.1",
    "font-awesome": "^4.7.0",
    "request-promise": "^4.2.5",
    "rxjs": "~6.5.4",
    "rxjs-compat": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngrx/schematics": "^9.0.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "@types/request-promise": "^4.1.44",
    "codelyzer": "^5.0.1",
    "electron": "^3.1.9",
    "electron-packager": "^13.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
